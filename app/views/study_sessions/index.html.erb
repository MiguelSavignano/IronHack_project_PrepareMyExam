<p id="notice"><%= notice %></p>

<h1>Listing Study Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Exam</th>
      <th>Theme</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_sessions.each do |study_session| %>
      <tr>
        <td><%= study_session.date %></td>
        <td><%= study_session.exam_id %></td>
        <td><%= study_session.theme_id %></td>
        <td><%= link_to 'Show', school_course_subject_exam_study_session_path(@school,@course,@subject,@exam,study_session) %></td>
        <td><%= link_to 'Edit', edit_school_course_subject_exam_study_session_path(@school,@course,@subject,@exam,study_session) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study session', new_school_course_subject_exam_study_session_path %>
